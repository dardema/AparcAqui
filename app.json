{
  "expo": {
    "name": "ProyFinal",
    "slug": "ProyFinal",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "plugins": [
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow $(PRODUCT_NAME) to use your location."
        }
      ]
    ],
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.bydestro12.ProyFinal",
      "infoPlist":{
        "NSLocationAlwaysAndWhenInUseUsageDescription": "Esta app utiliza la ubicación para crear un anuncio",
        "NSLocationAlwaysUsageDescription": "Esta app utiliza la ubicación para crear un anuncio",
        "NSLocationWhenInUseUsageDescription": "Esta app utiliza la ubicación para crear un anuncio"
      } 
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.bydestro12.ProyFinal"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "6d728c44-ee71-4575-9646-ced9bcec31be"
      } 
    },
  "scheme": "ProyFinal"

  }
}
